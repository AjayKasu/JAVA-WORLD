//problem statement: go through the problem direct question is a=1, b=2, n=10(a+2^n*b);
import java.util.*;
import java.io.*;

class Solution{
    public static void main(String []argh){
        Scanner in = new Scanner(System.in);
        int t=in.nextInt();
        for(int i=0;i<t;i++){
            int a = in.nextInt();
            int b = in.nextInt();
            int n = in.nextInt();
            for(int j=0;j<n;j++)
            {
                a=a+(int)Math.pow(2,j)*b;//Math.pow(,)here power values in decimal value so, it's to be int and precision values should be neglected
                System.out.print(a+" ");// inner for loop up to n.
            }
            System.out.println();//nextline after the inner for loop
        }
    }
}
